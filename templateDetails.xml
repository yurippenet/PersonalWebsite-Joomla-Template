<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE install PUBLIC "-//Joomla! 2.5//DTD template 1.0//EN" "http://www.joomla.org/xml/dtd/2.5/template-install.dtd">
<extension version="3.1" type="template" client="site">
	<name>Consult</name>
	<version>1.0</version>
	<creationDate>27/07/2015</creationDate>
	<author>Kristian Gausel</author>
	<authorEmail>yurippenet@gmail.com</authorEmail>
	<copyright>Copyright (C) 2015 Kristian.</copyright>
	<description>Template</description>
	<files>
		<filename>favicon.ico</filename>
		<filename>index.php</filename>
		<filename>index.html</filename>
		<filename>layout.html</filename>
		<filename>templateDetails.xml</filename>
		<folder>css</folder>
		<folder>html</folder>
		<folder>js</folder>
		<folder>fonts</folder>
		<folder>slick</folder>
	</files>
	<positions>
		<position>logo</position>
		<position>language-selection</position>
		<position>navbar</position>
		<position>position-1</position>
		<position>position-2</position>
		<position>position-3</position>
		<position>position-4</position>
		<position>position-5</position>
		<position>position-6</position>
		<position>position-7</position>
		<position>xpert-1</position>
		<position>xpert-2</position>
		<position>xpert-3</position>
		<position>xpert-4</position>
		<position>xpert-5</position>
		<position>xpert-6</position>
		<position>xpert-7</position>
		<position>xpert-8</position>
		<position>custom-article-start</position>
		<position>custom-article-end</position>
		<position>bottom-navbar</position>
		<position>footer</position>
		<position>customStyle</position>
		<position>customHead</position>
	</positions>
	<config>
		<fields name="params">
			<fieldset name="basic">
				<field name="sitetitle"  type="text" default=""
					label="Website Title"
					description="The name of your company"
					filter="string" />
				<field name="slickCarousel"
					type="radio"
					class="btn-group btn-group-yesno"
					default="1"
					label="Include Slick Carousel"
					description="Includes css and javascript for Slick Carousel"
				>
					<option value="1">JYES</option>
					<option value="0">JNO</option>
				</field>
			</fieldset>
		</fields>
	</config>
</extension>